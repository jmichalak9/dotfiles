*foreground: #00cc00
*background: #1b1918
*cursor: #a8a19f

!! URxvt
URxvt*scrollBar: false
URxvt.font: xft:monospace:size=11
URxvt.transparent: true
URxvt.shading: 7

!! black
*color0: #1b1918
*color8: #766e6b
!! red
*color1: #f22c40
*color9: #f22c40
!! green
*color2: #5ab738
*color10: #5ab738
!! yellow
*color3: #d5911a
*color11: #d5911a
!! blue
*color4: #407ee7
*color12: #407ee7
!! magenta
*color5: #6666ea
*color13: #6666ea
!! cyan
*color6: #00ad9c
*color14: #00ad9c
!! white
*color7: #a8a19f
*color15: #f1efee


!! Extensions
URxvt.perl-ext-common: default,resize-font
!! resize-font
URxvt.keysym.C-Up:      resize-font:bigger
URxvt.keysym.C-Down:     resize-font:smaller
